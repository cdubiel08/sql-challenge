# Physical Schema

employees
-
empl INT PK
em_title_id VARCHAR(255) FK - titles.title_id
birth_date DATE
first_name VARCHAR(255)
last_name VARCHAR(255)
sex VARCHAR(255)
hire_date DATE


salaries
-
emp_no INT FK - employees.empl
salary INT

titles
-
title_id VARCHAR(255) PK
title VARCHAR(255)

dept_manager
-
d VARCHAR(255) FK - departments.dept_no
emp_no INT FK - employees.empl

dept_emp
-
dept INT FK - employees.empl
dept_no VARCHAR(255) FK - departments.dept_no

departments
-
dept_no VARCHAR(255) PK
dept_name VARCHAR(255)
